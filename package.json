{
  "name": "dbuilder",
  "version": "1.0.1",
  "description": "Programatically build docker container",
  "main": "index.js",
  "scripts": {
    "build": "babel index.js -d dist",
    "watch": "chokidar index.js -c 'npm run build'",
    "watch-test": "mocha --watch",
    "lint": "npm run eslint && npm run jscs",
    "jscs": "jscs index.js test",
    "eslint": "eslint index.js test",
    "test": "npm run lint && mocha",
    "coverage": "istanbul cover _mocha -- test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markelog/dbuilder.git"
  },
  "keywords": [
    "docker",
    "build",
    "start",
    "attach",
    "container"
  ],
  "author": "Oleg Gaidarenko <markelog@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markelog/dbuilder/issues"
  },
  "homepage": "https://github.com/markelog/dbuilder",
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "dockerode": "^2.2.9",
    "dockerode-build": "^1.1.5",
    "pump": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.6.5",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.8",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.2",
    "eslint-plugin-react": "4.1.0",
    "gaze-cli": "^0.2.0",
    "istanbul": "^1.0.0-alpha.2",
    "jscs": "^2.11.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
